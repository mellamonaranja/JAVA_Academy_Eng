
public class JavaTest {
public static void main(String[] args) {
	
	int a=10;
	int b=20;
	
	boolean result;
	
	result = a<b;
	System.out.println("a < b :" + result);
	result = a>b;
	
	result = a<=b;
	
	result = a>=b;
	
	result = a==b;
	
	result = a!=b;
	
	
	System.out.println("a > b :" + result);
	System.out.println("a <= b :" + result);
	System.out.println("a >= b :" + result);
	System.out.println("a == b :" + result);
	System.out.println("a != b :" + result);
	
	
	
}
	
	
}
